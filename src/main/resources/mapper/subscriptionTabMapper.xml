<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.subscriptionTabDao">
	<insert id="insertSubTab" parameterType="com.entity.subscriptionTab">
		insert into subscription_tab(user_id,video_id,subscription_date) values(#{user_id,jdbcType=INTEGER},#{video_id,jdbcType=INTEGER},now())
	</insert>
</mapper>